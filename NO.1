class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        vector<int> result;
        for(int i=0;i<nums.size();i++)
        {
            int odd = target - nums[i];
            for(int j = i+1;j<nums.size();)
            {
                if(nums[j] != odd)
                    j++;
                else
                {
                    result.push_back(i);
                    result.push_back(j);
                    i=nums.size();
                    break;
                }
            }
            
        }
        return result;
        
    }
};
